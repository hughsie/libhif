<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE comps
  PUBLIC '-//Red Hat, Inc.//DTD Comps info//EN'
  'comps.dtd'>
<comps>
  <group>
    <id>standard</id>
    <name>Standard</name>
    <name xml:lang="cs">Standard</name>
    <name xml:lang="de">Standard</name>
    <name xml:lang="ja">標準</name>
    <description>Common set of utilities that extend the minimal installation.</description>
    <description xml:lang="cs">Běžná sada služeb rozšiřujících minimální instalaci.</description>
    <description xml:lang="de">Eine Sammlung häufig verwendeter Werkzeuge, welche die Minimalinstallation erweitern.</description>
    <description xml:lang="ja">最小限のインストールを拡張するユーティリティの共通セット</description>
    <display_order>1</display_order>
    <default>false</default>
    <uservisible>false</uservisible>
    <packagelist>
      <packagereq type="mandatory">cryptsetup</packagereq>
      <packagereq requires="gnome-control-center" type="conditional">chrony</packagereq>
      <packagereq requires="nonexistent" type="conditional">conditional</packagereq>
    </packagelist>
  </group>
</comps>
